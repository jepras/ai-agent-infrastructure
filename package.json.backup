{
  "name": "ai-email-processor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:backend": "pip install -r requirements.txt",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "uvicorn backend.main:app --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "uvicorn backend.main:app --reload --host 0.0.0.0 --port 8000"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
} 